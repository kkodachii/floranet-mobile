{
  "expo": {
    "scheme": "floranetmobile",
    "name": "floranet-mobile",
    "slug": "floranet-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.hyem23.floranetmobile",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.${ios.bundleIdentifier}.onesignal"
        ]
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/floranet.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hyem23.floranetmobile",
      "backgroundColor": "#f6f7f9"
    },
    "androidStatusBar": {
      "backgroundColor": "#f6f7f9",
      "barStyle": "dark-content",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-router",
      "expo-video"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ce22327e-86b7-4c70-a706-6ab70155ec93"
      }
    },
    "owner": "floranet"
  }
}